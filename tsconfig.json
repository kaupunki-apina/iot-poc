{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": ["es2018", "ESNext.AsyncIterable"],
    "allowJs": true,
    "checkJs": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types"],
    "isolatedModules": true
  },
  "exclude": ["node_modules", "**/node_modules/*", "cdk.out"]
}